<h1><%= @volunteer.name %></h1>

<form action="/volunteer/<%= @volunteer.id%>"  method="post">
  <div class= "form-group">
    <input name="_method" type = "hidden" value = "patch">

    <label for="volunteer_update">Update Volunteer: </label>
    <input id="volunteer_update" class="form-control" name="name" type="text">
  </div>
  <button type="submit" class="btn btn-primary">Update Volunteer</button>
</form>
